Cell["Test of energy conservation", "Subsection",
 CellChangeTimes->{{3.5997549562232733`*^9, 3.599754964897472*^9}, {
  3.5997876091403923`*^9, 3.5997876148432283`*^9}}],

Cell["\<\
Now let's calculate the total energy during the swing cycle, to verify that \
the Dyamics Workbench model conserves energy. Looking at the axes on the \
plot, it is evident that energy is conserved within machine precision\
\>", "Text",
 Evaluatable->False,
 AspectRatioFixed->True],

Cell[BoxData[{
 RowBox[{
  RowBox[{"KE", "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"Mass", "[", "a", "]"}], " ", 
     RowBox[{
      RowBox[{"VelCOM", "[", "a", "]"}], ".", 
      RowBox[{"VelCOM", "[", "a", "]"}]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"Mass", "[", "b", "]"}], " ", 
     RowBox[{
      RowBox[{"VelCOM", "[", "b", "]"}], ".", 
      RowBox[{"VelCOM", "[", "b", "]"}]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"Mass", "[", "c", "]"}], " ", 
     RowBox[{
      RowBox[{"VelCOM", "[", "c", "]"}], ".", 
      RowBox[{"VelCOM", "[", "c", "]"}]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{
      RowBox[{"Inertia", "[", "a", "]"}], ".", 
      RowBox[{"AngVel", "[", "a", "]"}], ".", 
      RowBox[{"AngVel", "[", "a", "]"}]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{
      RowBox[{"Inertia", "[", "b", "]"}], ".", 
      RowBox[{"AngVel", "[", "b", "]"}], ".", 
      RowBox[{"AngVel", "[", "b", "]"}]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{
      RowBox[{"Inertia", "[", "c", "]"}], ".", 
      RowBox[{"AngVel", "[", "c", "]"}], ".", 
      RowBox[{"AngVel", "[", "c", "]"}]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PE", " ", "=", " ", 
   RowBox[{"-", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"Mass", "[", "a", "]"}], " ", 
       RowBox[{
        RowBox[{"PosCOM", "[", "a", "]"}], ".", "grav"}]}], "+", 
      RowBox[{
       RowBox[{"Mass", "[", "b", "]"}], " ", 
       RowBox[{
        RowBox[{"PosCOM", "[", "b", "]"}], ".", "grav"}]}], "+", 
      RowBox[{
       RowBox[{"Mass", "[", "c", "]"}], " ", 
       RowBox[{
        RowBox[{"PosCOM", "[", "c", "]"}], ".", "grav"}]}]}], ")"}]}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.5997875635592117`*^9, 3.599787581153983*^9}, 
   3.5997876218295794`*^9, {3.599787660615054*^9, 3.599787661843587*^9}, {
   3.599787705831293*^9, 3.59978770806756*^9}}],

