  FontSize->12,
  FontWeight->"Bold"],
 StyleBox["joint_type",
  FontFamily->"Courier",
  FontSize->12,
  FontSlant->"Italic"],
 StyleBox[" ]",
  FontFamily->"Courier",
  FontSize->12,
  FontWeight->"Bold"],
 StyleBox[" ",
